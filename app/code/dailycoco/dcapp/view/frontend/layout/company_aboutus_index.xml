<page layout="1column" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
      <title>DailyCoco - About Us</title>
    </head>
    <body>
    	<referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">About Us</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
        	<block class="Dailycoco\Dcapp\Block\DailycocoAboutusBlock" name="Dcapp_productdelivery" template="Dailycoco_Dcapp::aboutus.phtml" />
        </referenceContainer>
        <referenceContainer name="content">
            <referenceBlock name="sidebar.main" remove="true"/>
            <referenceBlock name="sidebar.additional" remove="true"/>
        </referenceContainer >
    </body>
</page>